<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.0//EN"
    "http://www.w3.org/TR/2001/REC-SVG-20010904/DTD/svg10.dtd">

<svg width="100%" height="100%" viewBox="0 0 1280 720"
    xmlns="http://www.w3.org/2000/svg"
    xmlns:ekioh="http://www.ekioh.com/2007/ekioh"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:svg="http://www.w3.org/2000/svg" 
    image-rendering="optimizeSpeed">

	<%= javascript_include_tag "application", "data-turbolinks-track" => true %>

	<defs>
		<%= render :partial => 'gvt/defs' %>
		<%= yield :defs %>
		<style type="text/css" >
			<%= render :partial => 'gvt/styles' %>
			<%= yield :styles %>
		</style>
	</defs>

	<%= render :partial => 'gvt/header' %>
	<%= yield :header %>

<g	transform="translate(48 174)">
	<textArea	x="32"	y="-3"	width="400" class="gpbrbd">
	Minhas Ligações
	</textArea>
	<textArea	x="32"	y="64"	width="400" class="mmbrbd">
	Com o Minhas Ligações, você pode visualizar as chamadas recebidas, realizadas e perdidas do seu telefone da GVT na sua TV.
	</textArea>
	<textArea	x="32"	y="196"	width="400" class="mmbrbd">
	Digite a senha de bloqueio para visualizar o registro.
	</textArea>
</g>
	

<g id="senha" transform="translate(48 464)">

	<rect	x='0'	y='0'	width='464' height='96'	fill='url(#g160upbr)' fill-opacity="0.8" />
	
	<g id="senha" transform="translate(88 15)">
		<rect	x="0"	y="0"	width="66"	height="66" stroke-width="2" stroke="url(#ggryupdk)"	fill="url(#ggryupdk)"	fill-opacity="1" rx="6" ry="6" />
		<text	x="33"	y="66"	width="66"	text-anchor="middle" class="ggmbbd">*</text>
		<rect	x="74"	y="0"	width="66"	height="66" stroke-width="2" stroke="url(#ggryupdk)"	fill="#e5e5e5"			fill-opacity="1" rx="6" ry="6" />
		<text	x="81"	y="70"	width="66"	text-anchor="middle" class="ggmbbd"></text>
		<rect	x="148"	y="0"	width="66"	height="66" stroke-width="2" stroke="url(#ggryupdk)"	fill="url(#ggrydwbr)"	fill-opacity="0.8" rx="6" ry="6" />
		<text	x="181"	y="70"	width="66"	text-anchor="middle" class="ggmbbd"></text>
		<rect	x="222"	y="0"	width="66"	height="66" stroke-width="2" stroke="url(#ggryupdk)"	fill="url(#ggrydwbr)"	fill-opacity="0.8" rx="6" ry="6" />
		<text	x="255"	y="70"	width="66"	text-anchor="middle" class="ggmbbd"></text>
	</g>
	
</g>

<!--rodape-->
<g id="rodape" transform="translate(48 640)">
	<text	x="448"	y="32"	width="432" text-anchor="end" class="pmbrlg"></text>
	<rect	x='0'	y='-520'	width='8'	height='600'	fill='url(#shrg)' />
	<rect	x='456'	y='-520'	width='8'	height='600'	fill='url(#shlf)' />
</g>

<%= yield :footer %>
<%= render :partial => 'gvt/footer' %>